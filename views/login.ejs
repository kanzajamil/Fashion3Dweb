    <div class="at-loginsignupsection">
        <figure class="at-splashscreen">
            <img src="/Assets/images/splash-screen.jpg" alt="" />
        </figure>
        <div class="at-flexcontainer">
            <div class="flashing">
                <% if (flash) { %>
                    <div class="alert alert-<%-flash.type%>"><%- flash.message %></div>
                <% } %>
            </div>
            
            <div class="at-logoarea">
                <strong class="at-logo"><a href="javascript:void(0);"><img src="/Assets/images/logo.svg" alt=""></a></strong>
            </div>
            <div class="at-loginsignuparea">
                <form class="at-formtheme at-loginform" action="" method="post">
                    <fieldset>
                        <div class="form-group">
                            <legend class="">Welcome to Fashion 3D</legend>
                        </div>
                        <div class="form-group">
                            <label>Email</label>
                            <input type="text" name="email" class="form-control" placeholder="Enter your email" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Password</label>
                            <div class="at-inputwithicon">
                                <input type="password" name="password" class="form-control" placeholder="Enter Your password" autocomplete="off">
                                <i class="icon-eye-splash">
                                    <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M11.6854 9C11.6854 10.485 10.4854 11.685 9.00043 11.685C7.51543 11.685 6.31543 10.485 6.31543 9C6.31543 7.515 7.51543 6.315 9.00043 6.315C10.4854 6.315 11.6854 7.515 11.6854 9Z" stroke="white" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                                        <path d="M8.99988 15.2025C11.6474 15.2025 14.1149 13.6425 15.8324 10.9425C16.5074 9.885 16.5074 8.1075 15.8324 7.05C14.1149 4.35 11.6474 2.79 8.99988 2.79C6.35238 2.79 3.88488 4.35 2.16738 7.05C1.49238 8.1075 1.49238 9.885 2.16738 10.9425C3.88488 13.6425 6.35238 15.2025 8.99988 15.2025Z" stroke="white" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
                                    </svg>
                                </i>
                            </div>
                        </div>
                        <div class="form-group d-flex justify-content-between align-items-center">
                            <a href="javascript:void(0);" class="at-forgotpassword">Forgot Password?</a>
                        </div>
                        <div class="form-group text-center mb-2">
                            <button type="submit" class="at-btn at-btn-lg at-btnsignin login-spinner">
                                <span class="login-text">Sign In</span>
                                <i class="spinner-border spinner-border-sm d-none" id="loadderClass"></i>
                            </button>
                        </div>
                        <div class="at-withlogin">
                            <h2>Or login with</h2>
                        </div>
                        <div class="at-btngfarea">
                            <a href="/auth/google">
                            <button type="button" class="at-btn at-btngooglefacebook">
                                <svg width="23" height="22" viewBox="0 0 23 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M21.1258 11.2137C21.1258 10.4224 21.0603 9.84485 20.9185 9.24597H11.6973V12.8179H17.1099C17.0008 13.7055 16.4115 15.0424 15.102 15.9406L15.0836 16.0602L17.9992 18.2737L18.2012 18.2935C20.0563 16.6144 21.1258 14.144 21.1258 11.2137Z" fill="#4285F4"/>
                                    <path d="M11.6976 20.625C14.3494 20.625 16.5755 19.7694 18.2016 18.2936L15.1024 15.9408C14.273 16.5076 13.1599 16.9033 11.6976 16.9033C9.10043 16.9033 6.89609 15.2243 6.11031 12.9036L5.99513 12.9131L2.96347 15.2125L2.92383 15.3205C4.53888 18.4647 7.85634 20.625 11.6976 20.625Z" fill="#34A853"/>
                                    <path d="M6.10908 12.9036C5.90174 12.3047 5.78175 11.663 5.78175 11C5.78175 10.3369 5.90174 9.69524 6.09817 9.09636L6.09268 8.96881L3.02303 6.63257L2.92259 6.67938C2.25695 7.98412 1.875 9.44929 1.875 11C1.875 12.5507 2.25695 14.0158 2.92259 15.3205L6.10908 12.9036Z" fill="#FBBC05"/>
                                    <path d="M11.6977 5.09664C13.5419 5.09664 14.7859 5.87733 15.4953 6.52974L18.2671 3.8775C16.5648 2.32681 14.3494 1.375 11.6977 1.375C7.85637 1.375 4.53889 3.53526 2.92383 6.6794L6.09942 9.09638C6.89612 6.77569 9.10047 5.09664 11.6977 5.09664Z" fill="#EB4335"/>
                                </svg>		
                                <span class="login-text">Google</span>								
                            </button>
                            </a>
                        </div>
                    </fieldset>
                </form>
            </div>
            <div class="d-flex align-items-center justify-content-center at-accountyet">
                <p>Don't have an account yet? <a href="signup">Sign up</a></p>
            </div>
        </div>
    </div>

